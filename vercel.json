{
  "version": 2,
  "routes": [
    {
      "src": "/api/cities",
      "methods": ["GET"],
      "dest": "http://localhost:8000/cities"
    },
    {
      "src": "/api/cities/:id",
      "methods": ["GET"],
      "dest": "http://localhost:8000/cities/:id"
    },
    {
      "src": "/api/cities",
      "methods": ["POST"],
      "dest": "http://localhost:8000/cities"
    },
    {
      "src": "/api/cities/:id",
      "methods": ["DELETE"],
      "dest": "http://localhost:8000/cities/:id"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
